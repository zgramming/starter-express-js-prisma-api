{
  "name": "starter-express-js-prisma-api",
  "version": "1.0.0",
  "description": "Starter Express JS Prisma API",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts",
    "dev": "npx nodemon server.ts",
    "prisma:build": "npm run prisma:migrate && npm run prisma:generate && npm run prisma:format",
    "prisma:build-seeder": "npm run prisma:migrate && npm run prisma:seeder && npm run prisma:generate && npm run prisma:format",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:seeder": "ts-node prisma/seeds/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zgramming/starter-express-js-prisma-api.git"
  },
  "keywords": [
    "Express"
  ],
  "author": "Zeffry Reynando",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zgramming/starter-express-js-prisma-api/issues"
  },
  "homepage": "https://github.com/zgramming/starter-express-js-prisma-api#readme",
  "devDependencies": {
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/response-time": "^2.3.5",
    "@types/uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.3",
    "formidable": "^2.1.1",
    "jsonwebtoken": "^9.0.0",
    "response-time": "^2.3.2",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  }
}
